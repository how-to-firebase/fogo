{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "serve": "firebase serve --only functions",
    "shell": "firebase experimental:functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watchAll",
    "debug": "NODE_ENV=test node --inspect --debug-brk --nolazy ./node_modules/jest/bin/jest.js --runInBand",
    "version": "node --version",
    "protocol": "legacy"
  },
  "dependencies": {
    "@google-cloud/storage": "^2.4.3",
    "@quiver/firebase-utilities": "^0.0.5",
    "algoliasearch": "^3.32.1",
    "child_process": "^1.0.2",
    "cors": "^2.8.5",
    "exif-parser": "^0.1.12",
    "exif-reader": "^1.0.2",
    "fast-crc32c": "^1.0.4",
    "firebase-admin": "~7.2.0",
    "firebase-functions": "^2.2.1",
    "fs": "^0.0.1-security",
    "google-url": "^0.0.4",
    "path": "^0.12.7",
    "request": "^2.88.0"
  },
  "private": true,
  "devDependencies": {
    "jest": "^24.7.1",
    "node-mocks-http": "^1.7.3"
  },
  "engines": {
    "node": "8"
  }
}
